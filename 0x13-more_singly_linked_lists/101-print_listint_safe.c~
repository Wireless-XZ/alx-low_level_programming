#include "lists.h"

/**
 * print_listint_safe - prints a linked list
 * @head: head node
 *
 * returns the number of nodes
 */
size_t print_listint_safe(const listint_t *head)
{
	int count = 0;

	while (head)
	{
		printf("[%p] %d\n", (void *)head, head->n);
		head = head->next;
		count++;
	}
	return (count);
}
