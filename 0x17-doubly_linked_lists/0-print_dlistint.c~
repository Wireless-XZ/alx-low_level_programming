#include "lists.h"

/**
 * print_dlistint - prints all the elements of a doubly linked list
 * @h: head node
 *
 * Return: number of nodes in the list
 */
size_t print_dlistint(const dlistint_t *h)
{
	dlistint_t *dummy = h;
	long unsigned int len = 0;

	while(dummy)
	{
		printf("%d\n", dummy->n);
		dummy = dummy->next;
		len++;
	}

	return (len);
}
